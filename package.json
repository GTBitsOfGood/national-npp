{
  "name": "national-npp",
  "version": "1.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --fix --ext .jsx,.js,.tsx,.ts .",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss}\"",
    "secrets": "npm run secrets:logout && cross-env-shell BW_SESSION=`bw login product@bitsofgood.org --raw` \"npm run secrets:sync\"",
    "secrets:logout": "(bw logout || exit 0)",
    "secrets:login": "bw login product@bitsofgood.org",
    "secrets:sync": "bw sync && bw get item national-npp/development/.env.local | fx .notes > \".env.local\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.6.4",
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "chakra-ui-steps": "^1.3.0",
    "framer-motion": "^4.1.17",
    "mongodb": "^3.6.9",
    "mongoose": "^5.12.14",
    "next": "11.0.0",
    "next-auth": "^3.27.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.17.3",
    "react-icons": "^4.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/eslint-parser": "^7.14.7",
    "@bitwarden/cli": "^1.16.0",
    "@next/eslint-plugin-next": "^11.0.0",
    "@types/react": "17.0.11",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.29.0",
    "eslint-config-next": "^11.1.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "fx": "^20.0.2",
    "husky": "^6.0.0",
    "prettier": "^2.3.1",
    "typescript": "4.3.4"
  }
}
